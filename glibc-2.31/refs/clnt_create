<doc f='codebrowser/sunrpc/rpc/clnt.h' l='281'>/*
 * Generic client creation routine. Supported protocols are &quot;udp&quot;, &quot;tcp&quot; and
 * &quot;unix&quot;
 * CLIENT *
 * clnt_create(host, prog, vers, prot)
 *	char *host; 	-- hostname
 *	u_long prog;	-- program number
 *	u_ong vers;	-- version number
 *	char *prot;	-- protocol
 */</doc>
